@using OkulNot.Models.EntityFramework
@model List<NOTLAR>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>NOT ID</th>
        <th>OGRENCİ</th>
        <th>DERS</th>
        <th>SINAV1</th>
        <th>SINAV2</th>
        <th>SINAV3</th>
        <th>PROJE</th>
        <th>ORTALAMA</th>
        <th>DURUM</th>
        <th>GÜNCELLE</th>
    </tr>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.NOTID</td>
            <td>@item.OGRENCILER.OGRAD</td>
            <td>@item.DERSLER.DERSAD</td>
            <td>@item.SINAV1</td>
            <td>@item.SINAV2</td>
            <td>@item.SINAV3</td>
            <td>@item.PROJE</td>
            <td>@item.ORTALAMA</td>
            <td>@item.DURUM</td>
            <td>
                <a href="/Not/Guncelle/@item.NOTID" class="btn btn-warning">Güncelle</a>
            </td>

        </tr>
        }
    </tbody>
</table>
<a href="/Not/YeniSinav" class="btn btn-primary">Yeni Sınav</a>

